{
  "name": "devapt-core-browser",
  "version": "2.0.1",
  "description": "Developpers Application Toolkit - browser features: client runtime, ui rendering and management, client routing and logging",
  "author": {
    "name": "Luc BORIES",
    "email": "luc.bories@gmail.com",
    "url": "https://github.com/lucbories"
  },
  "contributors": [],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucbories/devapt-core-browser.git"
  },
  "bugs": {
    "url": "git+https://github.com/lucbories/devapt-core-browser/issues"
  },
  "homepage": "https://github.com/lucbories/devapt-core-browser#readme.md",
  "main": "dist/index.js",
  "engines": {
    "node": ">=4.4.4"
  },
  "directories": {
    "dist": "./dist"
  },
  "scripts": {
    "build": "gulp",
    "test": "mocha --recursive --reporter spec --compilers js:babel-register src/test",
    "postversion": "unix2dos package.json",
    "commit": "unix2dos package.json && git status && pause && git commit -am ",
    "patch": "npm version patch && unix2dos package.json && git status && pause && git commit -am ",
    "minor": "npm version minor && unix2dos package.json && git status && pause && git commit -am ",
    "major": "npm version major && unix2dos package.json && git status && pause && git commit -am ",
    "push-dev": "git push devapt develop",
    "merge": "git checkout master && git merge develop && git push devapt master && git checkout develop",
    "release": "git push devapt develop && git checkout master && git merge develop && git push devapt master && git checkout develop"
  },
  "browser": [
    "sequelize",
    "restify"
  ],
  "keywords": [
    "server",
    "api",
    "rest",
    "devapt",
    "toolkit",
    "redux",
    "reactive",
    "declarative",
    "distributed",
    "responsive"
  ],
  "dependencies": {
    "crossroads": ">=0.12.2",
    "devapt-core-common": ">=2.0.0",
    "dom-delegate": "^2.0.3",
    "es6-symbol": ">=3.1.0",
    "global": ">=4.3.1",
    "handlebars": ">=4.0.5",
    "hasher": ">=1.2.0",
    "html-to-vdom": ">=0.7.0",
    "immutable": ">=3.7.5",
    "jade": ">=1.11.0",
    "lodash": ">=4.16.4",
    "moment": ">=2.13.0",
    "mustache": ">=2.2.1",
    "object-sizeof": ">=1.0.10",
    "path-is-absolute": ">=1.0.0",
    "ramda": ">=0.21.0",
    "semver": ">=5.3.0",
    "socket.io": ">=1.3.7",
    "uuid": ">=3.0.0",
    "vdom-as-json": ">=1.0.9",
    "vdom-parser": ">=1.3.4",
    "vdom-virtualize": ">=2.0.0",
    "virtual-dom": ">=2.1.1"
  },
  "devDependencies": {
    "babel": ">=6.5.2",
    "babel-cli": ">=6.6.4",
    "babel-loader": ">=6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
